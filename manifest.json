{
  "name": "Wallbase Lazy Download",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_scripts": [{
        "js": ["src/inject/inject.js", "js/jquery/jquery.min.js"],
        "matches": ["*://*.wallbase.cc/*"]
    }],
  "description": "Easily download images from wallbase.cc",
  "minimum_chrome_version": "16.0.884",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": ["downloads", "tabs", "background", "*://*.wallbase.cc/*"],
  "page_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "lazy download"
  }  
}